<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <default type="label">
        <align>left</align>
        <aligny>center</aligny>
        <font>InfoLine</font>
        <textcolor>TextBright</textcolor>
        <selectedcolor>TextBright</selectedcolor>
        <scrollspeed>80</scrollspeed>
        <scrollsuffix>  •  </scrollsuffix>
    </default>

    <default type="button">
        <scrollspeed>80</scrollspeed>
        <scrollsuffix>  •  </scrollsuffix>
        <pulseonselect>false</pulseonselect>
    </default>
    
    <default type="togglebutton">
        <scrollspeed>80</scrollspeed>
        <scrollsuffix>  •  </scrollsuffix>
        <pulseonselect>false</pulseonselect>
    </default>

    <default type="textbox">
        <scrollspeed>80</scrollspeed>
        <scrollsuffix>  •  </scrollsuffix>
    </default>

    <default type="radiobutton">
        <pulseonselect>false</pulseonselect>
        <textureradioonfocus>buttons/RadioOnFocus.png</textureradioonfocus>
        <textureradioonnofocus>buttons/RadioOnNoFocus.png</textureradioonnofocus>
        <textureradioofffocus>buttons/RadioOffFocus.png</textureradioofffocus>
        <textureradiooffnofocus>buttons/RadioOffNoFocus.png</textureradiooffnofocus>
        <textureradioondisabled colordiffuse="TextMid">buttons/RadioOn.png</textureradioondisabled>
        <textureradiooffdisabled colordiffuse="TextMid">buttons/RadioOff.png</textureradiooffdisabled>
        <radioposx></radioposx>
        <radioposy>0</radioposy>
        <radiowidth>164</radiowidth>
        <radioheight>104</radioheight>
    </default>


    <default type="grouplist">
        <itemgap>0</itemgap>
        <scrolltime tween="cubic" easing="out">500</scrolltime>
    </default>

    <default type="list">
        <scrolltime tween="cubic" easing="out">500</scrolltime>
    </default>

    <default type="wraplist">
        <scrolltime tween="cubic" easing="out">500</scrolltime>
    </default>

    <default type="fixedlist">
        <scrolltime tween="cubic" easing="out">500</scrolltime>
    </default>

    <default type="panel">
        <scrolltime tween="cubic" easing="out">500</scrolltime>
    </default>
</includes>